{
  "name": "telnix-wd",
  "type": "module",
  "devDependencies": {
    "@wdio/allure-reporter": "^9.12.3",
    "@wdio/cli": "^9.12.5",
    "@wdio/local-runner": "^9.12.5",
    "@wdio/mocha-framework": "^9.12.5",
    "@wdio/spec-reporter": "^9.12.3"
  },
  "dependencies": {
    "@faker-js/faker": "^9.7.0"
  },
  "scripts": {
